[globcfg]
data_width = 32
address_width = 32
address_alignment = data_width
force_name_case = none

[c_header]
path = ./generated/usb_hid_regs.h
prefix = USB_HID
generator = ./generators/c_nocstd_generator.py::CHeaderNoCstd

[md_doc]
path = ./generated/usb_hid_regs.md
title = USB HID Register map
print_images = True
image_dir = md_img
print_conventions = True
generator = Markdown

[asm]
path = ./generated/usb_hid_regs.s
prefix = USB_HID
generator = ./generators/asm_generator.py::AsmGenerator
