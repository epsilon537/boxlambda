[globcfg]
base_address = 0x10010000
data_width = 32
address_width = 32
address_alignment = data_width
force_name_case = none

[c_header]
path = ./generated/uart_regs.h
prefix = UART
generator = ./generators/c_nocstd_generator.py::CHeaderNoCstd

[md_doc]
path = ./generated/uart_regs.md
title = UART Register map
print_images = True
image_dir = md_img
print_conventions = True
generator = Markdown

[asm]
path = ./generated/uart_regs.s
prefix = UART
generator = ./generators/asm_generator.py::AsmGenerator
