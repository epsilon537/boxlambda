[globcfg]
base_address = 0x10001000
data_width = 32
address_width = 32
address_alignment = data_width
force_name_case = none

[c_header]
path = ./generated/ym2149_regs.h
prefix = YM2149
generator = ./generators/c_nocstd_generator.py::CHeaderNoCstd

[md_doc]
path = ./generated/ym2149_regs.md
title = YM2149 Register map
print_images = True
image_dir = md_img
print_conventions = True
generator = Markdown

[asm]
path = ./generated/ym2149_regs.s
prefix = YM2149
generator = ./generators/asm_generator.py::AsmGenerator
