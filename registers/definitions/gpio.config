[globcfg]
base_address = 0x10000100
data_width = 32
address_width = 32
address_alignment = data_width
force_name_case = none

[c_header]
path = ./generated/gpio_regs.h
prefix = GPIO
generator = ./generators/c_nocstd_generator.py::CHeaderNoCstd

[md_doc]
path = ./generated/gpio_regs.md
title = GPIO Register map
print_images = True
image_dir = md_img
print_conventions = True
generator = Markdown

[asm]
path = ./generated/gpio_regs.s
prefix = GPIO
generator = ./generators/asm_generator.py::AsmGenerator
