[globcfg]
base_address = 0x10000400
data_width = 32
address_width = 32
address_alignment = data_width
force_name_case = none

[c_header]
path = ./generated/dfx_controller_regs.h
prefix = DFX
generator = ./generators/c_nocstd_generator.py::CHeaderNoCstd

[md_doc]
path = ./generated/dfx_controller_regs.md
title = DFX Controller Register map
print_images = True
image_dir = md_img
print_conventions = True
generator = Markdown

[asm]
path = ./generated/dfx_controller_regs.s
prefix = DFX
generator = ./generators/asm_generator.py::AsmGenerator
