[globcfg]
base_address = 0x10000020
data_width = 32
address_width = 32
address_alignment = data_width
force_name_case = none

[c_header]
path = ./generated/sdspi_regs.h
prefix = SDSPI
generator = ./generators/c_nocstd_generator.py::CHeaderNoCstd

[md_doc]
path = ./generated/sdspi_regs.md
title = SDSPI Register map
print_images = True
image_dir = md_img
print_conventions = True
generator = Markdown

[asm]
path = ./generated/sdspi_regs.s
prefix = SDSPI
generator = ./generators/asm_generator.py::AsmGenerator
